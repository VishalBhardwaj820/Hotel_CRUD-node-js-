
<form action="/create" method="POST">
    <label for="name">Name</label><br>
    <input type="text" name="name" id="name"><br><br>

    <label for="email">Email</label><br>
    <input type="text" name="email" id="email"><br><br>

    <label for="phone">Moblie Number</label><br>
    <input type="text" name="phone" id="phone"><br><br>

    <label for="address">Addresss</label><br>
    <input type="text" name="address" id="address"><br><br>

    <label for="members">No of Members</label><br>
    <input type="text" name="members" id="members"><br><br>

    <label for="food">Food Type</label><br>
    <select name="food" id="food">
        <option value="South Indian">South Indian</option>
        <option value="North Indian">North Indian</option>
    </select><br><br>

    <label for="facility">Free Extra Facility</label><br>
    <select name="facility" id="facility">
        <option value="gym">Gym</option>
        <option value="laoundry">Loundry</option>
        <option value="indoorgame">Indoor Games</option>
    </select>

    <br><br>

    <label for="payment">Payment Mode</label><br>
    <select name="payment" id="payment">
        <option value="googlepay">Google Pay</option>
        <option value="paytm">Paytm</option>
        <option value="card ">Card</option>
    </select><br><br>

    <label for="amount">Amount to be Paid</label><br><br>
    <input type="text" name="amount" id="amount">

    <input type="submit">
</form>